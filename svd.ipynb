      " [ 2.1  3.2  4.1  5.2]]\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "from numpy.linalg import svd\n",
    "\n",
    "X = np.array([\n",
    "    [2, 3, 4, 5],\n",
    "    [2.2, 3.1, 4.2, 5.3],\n",
    "    [2.1, 3.2, 4.1, 5.2],\n",
    "])\n",
    "\n",
    "U, s, V = svd(X, full_matrices = False)\n",
    "D = np.diag(s)\n",
    "V = V.T\n",
    "\n",
    "print X\n",
    "print U.dot(D).dot(V.T)"
   ]
  }
